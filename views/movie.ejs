
<% include partials/header %>

    <!-- Movie details -->
    <div class="container mt-5">
        <h2 class="my-3"> <%= movie.title %></h2>
        <!-- Description -->
        <h5 class="font-weight-bold"> Description</h5>
        <p> <%= movie.description %> </p>
        <!-- Release date -->
        <h5 class="font-weight-bold">Release date</h5>
        <p> <%= movie.release_date %> </p>
        <!-- Director -->
        <h5 class="font-weight-bold"> Director</h5>
        <p> <%= movie.director %> </p>
        <!-- Producer -->
        <h5 class="font-weight-bold"> Producer</h5>
        <p> <%= movie.producer %> </p>
        <!-- Title -->
        <h5 class="font-weight-bold"> Rotten Tomatoes Score</h5>
        <p> <%= movie.rt_score %> </p>
        
        
    </div>

    


